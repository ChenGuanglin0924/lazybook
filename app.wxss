@import './styles/iconfont.wxss';

/**app.wxss**/
.container {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  margin: 0;
  padding: 0;
  display: flex;
  flex-direction: column;
  align-content: flex-start;
  align-items: center;
  box-sizing: border-box;
  background-color: #e9e9e9;
}

/*去边框button  类似于a标签*/
.a {
  background-color: transparent;
  padding:0;
  margin:0;
  font-size: 14px;
  line-height: 14px;
  border-radius:0;
}

.a:hover {
  opacity: .8;
}

.a::after{
  border:none;
}